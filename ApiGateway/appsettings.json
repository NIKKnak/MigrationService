{
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/auth" }
        ]
      },
      "currency-route": {
        "ClusterId": "currency-cluster",
        "Match": {
          "Path": "/currency/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/currency" }
        ]
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-service": { "Address": "http://localhost:5034/" }
        }
      },
      "currency-cluster": {
        "Destinations": {
          "currency-service": { "Address": "http://localhost:5132/" }
        }
      }
    }
  },
  "Jwt": {
    "Key": "vfsdlpgjdgfd,gl;fdjgdfogfmdyjgdfspgjdf[odasutidhgjsdfotijdf0g",
    "Issuer": "UserService",
    "Audience": "UserServiceAudience"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
